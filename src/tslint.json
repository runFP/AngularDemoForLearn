{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      ["app","test"],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      ["app","test"],
      "kebab-case"
    ],
    "no-inferrable-types": [
      true,
      "ignore-params",
      "ignore-properties"
    ]
  }
}
